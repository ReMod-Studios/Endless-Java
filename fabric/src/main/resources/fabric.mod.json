{
  "schemaVersion": 1,
  "id": "endless",
  "version": "${version}",
  "name": "Endless",
  "description": "An End Expansion aiming to add more wackyness to the End Dimension!",
  "authors": [
    "ReMod Studios"
  ],
  "license": "Apache-2.0",
  "icon": "assets/endless/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [{
      "adapter": "kotlin",
      "value": "com.remodstudios.endless.fabric.EndlessFabric"
    }],
    "client": [{
      "adapter": "kotlin",
      "value": "com.remodstudios.endless.fabric.EndlessFabricClient"
    }]
  },
  "mixins": [
    "endless.common.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.11.1",
    "fabric": "*",
    "architectury": ">=1.1.55",
    "fabric-language-kotlin": ">=1.6.0+kotlin.1.5.0"
  }
}